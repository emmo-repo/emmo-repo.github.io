    ns2:prefLabel "Index"@en .

emmo:MassiveElementary a ns1:Class ;
    emmo:definition "The union of classes of elementary particles that possess mass."@en ;
    emmo:elucidation "An elementary particle that pocess mass."@en ;
    rdfs:isDefinedBy <http://emmo.info/emmo#>,
        <http://emmo.info/emmo/middle/physicalistic#> ;
    rdfs:subClassOf emmo:StandardModelParticle ;
    ns1:equivalentClass [ a ns1:Class ;
            ns1:unionOf ( emmo:WBoson emmo:HiggsBoson emmo:Quark emmo:Lepton emmo:ZBoson ) ] ;
    ns2:exactMatch "http://emmo.info/emmo#EMMO_385b8f6e_43ac_4596_ad76_ac322c68b7ca" ;
    ns2:prefLabel "MassiveElementary"@en .

emmo:Matter a ns1:Class ;
    emmo:elucidation "A 'Physical' that possesses some 'Lepton' or 'Quark' parts in each of its temporal parts."@en ;
    rdfs:isDefinedBy <http://emmo.info/emmo#>,
        <http://emmo.info/emmo/middle/physicalistic#> ;
    rdfs:subClassOf [ a ns1:Restriction ;
