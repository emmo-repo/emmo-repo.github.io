            ns1:unionOf ( emmo:EMMO_c5ae6d8e_6b39_431f_8de4_ae4e357abc04 emmo:EMMO_dd14d055_2db0_4b81_bc97_ef6c2f72b8a0 emmo:EMMO_e1021593_06da_4237_8a02_29d8f6fef76d emmo:EMMO_4b3afb22_27cf_4ce3_88bc_492bfccb546b emmo:EMMO_8f207971_aaab_48dc_a10d_55a6b4331410 ) ] ;
