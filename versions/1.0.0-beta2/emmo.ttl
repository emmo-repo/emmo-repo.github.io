        [ a ns1:Restriction ;
            ns1:hasValue emmo:EMMO_08cb807c_e626_447b_863f_e2835540e918 ;
            ns1:onProperty [ ns1:inverseOf emmo:EMMO_17e27c22_37e1_468c_9dd7_95e137f73e7f ] ] ;
