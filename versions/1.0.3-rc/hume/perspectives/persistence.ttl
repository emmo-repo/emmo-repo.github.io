ns1:isRemovedFrom a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "isRemovedFrom"@en ;
    ns2:python_name "isRemovedFrom"@en ;
    rdfs:comment "The relation between an object O1 and a object O2 that proper overlap, where the non overapping part of O2 is caused by O1."@en ;
    rdfs:domain ns1:Object ;
    rdfs:range ns1:Object ;
    rdfs:subPropertyOf ns1:leaves ;
    owl:inverseOf ns1:releases ;
    skos:prefLabel "isRemovedFrom"@en ;
    ns1:elucidation "The relation between an object O1 and a object O2 that proper overlap, where the non overapping part of O2 is caused by O1."@en .

: a owl:Ontology ;
    dcterms:abstract ""@en ;
    dcterms:contributor <https://orcid.org/GerhardGoldbeck>,
        <https://orcid.org/JesperFriis> ;
    dcterms:creator <https://orcid.org/EmanueleGhedini> ;
    dcterms:license "https://creativecommons.org/licenses/by/4.0/legalcode"^^xsd:string ;
    dcterms:publisher <https://emmc.eu> ;
    dcterms:title "Persistence"@en ;
    rdfs:comment <https://orcid.org/FrancescoZaccarini>,
        "The EMMO requires HermiT reasoner plugin in order to visualize all inferences and class hierarchy (ctrl+R hotkey in Protege)."@en ;
    owl:imports <https://w3id.org/emmo/1.0.3/perspectives/perspective> ;
    owl:versionIRI <https://w3id.org/emmo/1.0.3/hume/perspectives/persistence> ;
    owl:versionInfo "1.0.3"^^xsd:string ;
    ns1:contact "emmo@emmc.eu"^^xsd:string .

ns1:Behaviour a owl:Class ;
    rdfs:label "Behaviour"@en ;
    rdfs:comment "A process which is a temporal part of an object."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf ns1:hasBehaviour ] ;
            owl:someValuesFrom ns1:Object ],
        ns1:ProcessByContext ;
    skos:example "Accelerating is a behaviour of a car."@en ;
    skos:prefLabel "Behaviour"@en ;
    ns1:elucidation "A process which is a temporal part of an object."@en .

ns1:Constituent a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf ns1:hasConstituent ] ;
            owl:someValuesFrom ns1:Object ],
        ns1:ObjectByContext ;
    skos:altLabel "ObjectPart"@en ;
    skos:example "A tire is a constituent of a car."@en ;
    skos:prefLabel "Constituent"@en ;
    ns1:elucidation "An object which is a spatial part of another object."@en .

ns1:ConstitutiveProcess a owl:Class ;
    rdfs:label "ConstitutiveProcess"@en ;
    rdfs:comment "A process which is a spatial part of an object."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf ns1:hasConstitutiveProcess ] ;
            owl:someValuesFrom ns1:Object ],
        ns1:ProcessByContext ;
    skos:example "Blood circulation in a human body."@en ;
    skos:prefLabel "ConstitutiveProcess"@en ;
    ns1:elucidation "A process which is a spatial part of an object."@en ;
    ns1:note "A constitutive process is a process that is holistically relevant for the definition of the whole."@en .

ns1:Participant a owl:Class ;
    rdfs:label "Participant"@en ;
    rdfs:comment "An object which is a spatial part of a process."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf ns1:hasParticipant ] ;
            owl:someValuesFrom ns1:Process ],
        ns1:ObjectByContext ;
    skos:example "A student during an examination."@en ;
    skos:prefLabel "Participant"@en ;
    ns1:elucidation "An object which is a spatial part of a process."@en .

ns1:Stage a owl:Class ;
    rdfs:label "Stage"@en ;
    rdfs:comment "A process which is a temporal part of another process."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf ns1:hasStage ] ;
            owl:someValuesFrom ns1:Process ],
        ns1:ProcessByContext ;
    skos:example "Moving a leg is a stage of the process of running."@en ;
    skos:prefLabel "Stage"@en ;
    ns1:elucidation "A process which is a temporal part of another process."@en .

ns1:Status a owl:Class ;
    rdfs:label "Status"@en ;
    rdfs:comment "An object which is a temporal part of a process."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf ns1:hasStatus ] ;
            owl:someValuesFrom ns1:Process ],
        ns1:ObjectByContext ;
    skos:altLabel "State"@en ;
    skos:example "A semi-naked man is a status in the process of a man's dressing."@en ;
    skos:prefLabel "Status"@en ;
    ns1:elucidation "An object which is a temporal part of a process."@en .

ns1:SubObject a owl:Class ;
    rdfs:label "SubObject"@en ;
    rdfs:comment "An object which is a temporal part of another object."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf ns1:hasSubObject ] ;
            owl:someValuesFrom ns1:Object ],
        ns1:ObjectByContext ;
    skos:example "If an inhabited house is considered as an house that is occupied by some people in its majority of time, then an interval of inhabited house in which occasionally nobody is in there is no more an inhabited house, but an unhinabited house, since this temporal part does not satisfy the criteria of the whole."@en ;
    skos:prefLabel "SubObject"@en ;
    ns1:elucidation "An object which is a temporal part of another object."@en ;
    ns1:note "Here we consider a temporal interval that is lower than the characteristic time of the physical process that provides the causality connection between the object parts."@en .

ns1:SubProcess a owl:Class ;
    rdfs:label "SubProcess"@en ;
    rdfs:comment "A process which is a spatial part of another process."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf ns1:hasSubProcess ] ;
            owl:someValuesFrom ns1:Process ],
        ns1:ProcessByContext ;
    skos:example "Breathing is a subprocess of living for a human being."@en ;
    skos:prefLabel "SubProcess"@en ;
    ns1:elucidation "A process which is a spatial part of another process."@en ;
    ns1:note """In the EMMO the relation of participation to a process falls under mereotopology.

Since topological connection means causality, then the only way for a real world object to participate to a process is to be a part of it."""@en .

ns1:contributesTo a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "contributesTo"@en ;
    ns2:python_name "contributesTo"@en ;
    rdfs:comment "The relation between a process P1 that through overlaps a process P2."@en ;
    rdfs:domain ns1:Process ;
    rdfs:range ns1:Process ;
    rdfs:subPropertyOf ns1:throughOverlaps ;
    skos:prefLabel "contributesTo"@en ;
    ns1:elucidation "The relation between a process P1 that through overlaps a process P2."@en ;
    ns1:illustration "https://raw.githubusercontent.com/emmo-repo/EMMO/dev/doc/figs/contributesTo.png"^^xsd:string .

ns1:hasBehaviour a owl:ObjectProperty ;
    rdfs:label "hasBehaviour"@en ;
    ns2:python_name "hasBehaviour"@en ;
    rdfs:domain ns1:Object ;
    rdfs:range ns1:Behaviour ;
    rdfs:subPropertyOf ns1:hasTemporalPart ;
    skos:prefLabel "hasBehaviour"@en .

ns1:hasConstituent a owl:ObjectProperty ;
    rdfs:label "hasConstituent"@en ;
    ns2:python_name "hasConstituent"@en ;
    rdfs:comment "The relation between an object and one of its holistic part that contributes to the object under some spatial-based criteria."@en ;
    rdfs:domain ns1:Object ;
    rdfs:range ns1:Constituent ;
    rdfs:subPropertyOf ns1:hasNonTemporalPart ;
    skos:prefLabel "hasConstituent"@en ;
    ns1:elucidation "The relation between an object and one of its holistic part that contributes to the object under some spatial-based criteria."@en .

ns1:hasConstitutiveProcess a owl:ObjectProperty ;
    rdfs:label "hasConstitutiveProcess"@en ;
    ns2:python_name "hasConstitutiveProcess"@en ;
    rdfs:comment "Relates an object to one of its constitutive process parts that contributes to the object under some spatial-based criteria."@en ;
    rdfs:domain ns1:Object ;
    rdfs:range ns1:ConstitutiveProcess ;
    rdfs:subPropertyOf ns1:hasNonTemporalPart ;
    skos:prefLabel "hasConstitutiveProcess"@en ;
    ns1:elucidation "Relates an object to one of its constitutive process parts that contributes to the object under some spatial-based criteria."@en .

ns1:hasObjectOutput a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "hasObjectOutput"@en ;
    ns2:python_name "hasObjectOutput"@en ;
    rdfs:comment "The relation between a process P that proper overlaps an object O, where the non-overlapping part of O is caused by P."@en ;
    rdfs:range ns1:Object ;
    rdfs:subPropertyOf ns1:hasOutput ;
    skos:prefLabel "hasObjectOutput"@en ;
    ns1:elucidation "The relation between a process P that proper overlaps an object O, where the non-overlapping part of O is caused by P."@en ;
    ns1:illustration "https://raw.githubusercontent.com/emmo-repo/EMMO/dev/doc/figs/hasObjectOutput.png"^^xsd:string .

ns1:hasParticipant a owl:ObjectProperty ;
    rdfs:label "hasParticipant"@en ;
    ns2:python_name "hasParticipant"@en ;
    rdfs:comment """Participation is a parthood relation: you must be part of the process to contribute to it. A participant whose 4D extension is totally contained within the process.

Participation is not under direct parthood since a process is not strictly related to reductionism, but it's a way to categorise temporal regions by the interpreters."""@en,
        "The relation between a process and an object participating to it, i.e. that is relevant to the process itself."@en ;
    rdfs:domain ns1:Process ;
    rdfs:range ns1:Participant ;
    rdfs:subPropertyOf ns1:hasNonTemporalPart ;
    skos:prefLabel "hasParticipant"@en ;
    ns1:elucidation "The relation between a process and an object participating to it, i.e. that is relevant to the process itself."@en .

ns1:hasProcessInput a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "hasProcessInput"@en ;
    ns2:python_name "hasProcessInput"@en ;
    rdfs:comment "The relation between a process P1 that proper overlaps a process P2, where the non-overlapping part of P2 causes P1."@en ;
    rdfs:range ns1:Process ;
    rdfs:subPropertyOf ns1:hasInput ;
    skos:prefLabel "hasProcessInput"@en ;
    ns1:elucidation "The relation between a process P1 that proper overlaps a process P2, where the non-overlapping part of P2 causes P1."@en ;
    ns1:illustration "https://raw.githubusercontent.com/emmo-repo/EMMO/dev/doc/figs/hasProcessInput.png"^^xsd:string .

ns1:hasProcessOutput a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "hasProcessOutput"@en ;
    ns2:python_name "hasProcessOutput"@en ;
    rdfs:comment "The relation between a process P1 that proper overlaps a process P2, where the non-overlapping part of P2 is caused by P1."@en ;
    rdfs:range ns1:Process ;
    rdfs:subPropertyOf ns1:hasOutput ;
