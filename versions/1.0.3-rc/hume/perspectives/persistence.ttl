ns1:affects a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "affects"@en ;
    ns2:python_name "affects"@en ;
    rdfs:comment "The relation between a process P that through overlaps an object O."@en ;
    rdfs:domain ns1:Process ;
    rdfs:range ns1:Object ;
    rdfs:subPropertyOf ns1:throughOverlaps ;
    owl:inverseOf ns1:isAffectedBy ;
    skos:prefLabel "affects"@en ;
    ns1:elucidation "The relation between a process P that through overlaps an object O."@en ;
    ns1:illustration "https://raw.githubusercontent.com/emmo-repo/EMMO/dev/doc/figs/affects.png"^^xsd:string .

ns1:generates a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "generates"@en ;
    ns2:python_name "generates"@en ;
    rdfs:comment "The relation between an object O that proper overlaps a process P, where the non-overlapping part of P is caused by O."@en ;
    rdfs:range ns1:Process ;
    rdfs:subPropertyOf ns1:isLeftBy ;
    owl:inverseOf ns1:isGeneratedBy ;
    skos:prefLabel "generates"@en ;
    ns1:elucidation "The relation between an object O that proper overlaps a process P, where the non-overlapping part of P is caused by O."@en ;
    ns1:illustration "https://raw.githubusercontent.com/emmo-repo/EMMO/dev/doc/figs/generates.png"^^xsd:string .

