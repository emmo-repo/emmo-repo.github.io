    rdfs:label "notOverlaps"@en ;
    ns1:python_name "notOverlaps"@en ;
    rdfs:comment "Dxy ≡ ¬Oxy."^^xsd:string,
        "The relation between individuals representing non-overlapping entities (i.e. that have no parts in common)."@en ;
