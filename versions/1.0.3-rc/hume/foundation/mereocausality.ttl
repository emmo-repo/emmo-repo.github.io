    skos:prefLabel "notOverlaps"@en ;
    ns2:definition "Dxy ≡ ¬Oxy."^^xsd:string ;
    ns2:elucidation "The relation between individuals representing non-overlapping entities (i.e. that have no parts in common)."@en .

ns2:successorOf a owl:AsymmetricProperty,
