ns2:hasFragment a owl:ObjectProperty ;
    rdfs:label "hasFragment"@en ;
    ns1:python_name "hasFragment"@en ;
    rdfs:comment "The relation between two individuals representing a collection and a non-maximal proper part, meaning the the latter is connected with the rest of the whole."@en ;
    rdfs:domain ns2:Collection ;
    rdfs:subPropertyOf ns2:hasPiece ;
    owl:inverseOf ns2:isFragmentOf ;
    skos:prefLabel "hasFragment"@en ;
    ns2:elucidation "The relation between two individuals representing a collection and a non-maximal proper part, meaning the the latter is connected with the rest of the whole."@en .

ns2:hasJunctionPart a owl:ObjectProperty ;
    rdfs:label "hasJunctionPart"@en ;
    ns1:python_name "hasJunctionPart"@en ;
    rdfs:comment "The part is connected with the rest item or members with both hasNext (or its inverse) and hasContact relations."@en ;
    rdfs:domain ns2:CausalSystem ;
    rdfs:subPropertyOf ns2:hasNonTemporalPart ;
    owl:inverseOf ns2:isJunctionPartOf ;
    owl:propertyDisjointWith ns2:hasSpatialPart ;
    skos:altLabel "hasSpatioTemporalPart"@en ;
    skos:prefLabel "hasJunctionPart"@en ;
    ns2:elucidation "The part is connected with the rest item or members with both hasNext (or its inverse) and hasContact relations."@en .

ns2:illustration a owl:AnnotationProperty ;
    rdfs:label "illustration"@en ;
    ns1:python_name "illustration"@en ;
    rdfs:comment "A link to a graphical representation aimed to facilitate understanding of the concept, or of an annotation."@en ;
    rdfs:range xsd:anyURI ;
    rdfs:subPropertyOf rdfs:seeAlso ;
    skos:prefLabel "illustration"@en ;
    ns2:elucidation "A link to a graphical representation aimed to facilitate understanding of the concept, or of an annotation."@en .

ns2:isMemberOf a owl:ObjectProperty ;
