ns1:EMMO_eb77076b_a104_42ac_a065_798b2d2809ad a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ns1:EMMO_dc57d998_23db_4d8e_b2cd_f346b195b846 ;
            owl:someValuesFrom ns1:EMMO_f835f4d4_c665_403d_ab25_dca5cc74be52 ],
        [ a owl:Restriction ;
            owl:onProperty ns1:EMMO_dc57d998_23db_4d8e_b2cd_f346b195b846 ;
