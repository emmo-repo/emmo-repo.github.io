@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ns1: <http://www.lesfleursdunormal.fr/static/_downloads/owlready_ontology.owl#> .
@prefix ns2: <https://w3id.org/emmo#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

owl:qualifiedCardinality a owl:AnnotationProperty .

ns2:Measurer a owl:Class ;
    rdfs:subClassOf ns2:Observer ;
    skos:prefLabel "Measurer"@en ;
    ns2:elucidation "An observer that makes use of a measurement tool and provides a quantitative property."@en .

ns2:Modeller a owl:Class ;
    rdfs:label "Modeller"@en ;
    rdfs:comment "A estimator that uses modelling to declare a property of an object (i.e. infer a property from other properties)."@en ;
    rdfs:subClassOf ns2:Estimator ;
    skos:prefLabel "Modeller"@en ;
    ns2:elucidation "A estimator that uses modelling to declare a property of an object (i.e. infer a property from other properties)."@en .

ns2:hasCharacterised a owl:ObjectProperty ;
    rdfs:label "hasCharacterised"@en ;
    ns1:python_name "hasCharacterised"@en ;
    rdfs:domain ns2:Determiner ;
    rdfs:subPropertyOf ns2:hasDeclared ;
    skos:prefLabel "hasCharacterised"@en .

ns2:hasCharacteriser a owl:ObjectProperty ;
    rdfs:label "hasCharacteriser"@en ;
    ns1:python_name "hasCharacteriser"@en ;
    rdfs:domain ns2:Property ;
    rdfs:range ns2:Determiner ;
    rdfs:subPropertyOf ns2:hasDeclarer ;
    skos:prefLabel "hasCharacteriser"@en .

<https://w3id.org/emmo/disciplines/properties> a owl:Ontology ;
    dcterms:abstract """Defines properties as the result of an observation process. Observation is a semiotic process that stimulate an interpretant within the interpreter who communicates the observed result to other interpreters through a sign, which is the property.

Hence, properties creates a link between the holistic and the symbolic perspectives.

Subclasses of property are subclasses that spesialise the type of observation process."""@en ;
    dcterms:creator <https://orcid.org/0000-0003-4065-9742>,
        ns2:AdhamHashibon,
        ns2:EmanueleGhedini,
        ns2:GerhardGoldbeck,
        ns2:JesperFriis ;
    dcterms:license "https://creativecommons.org/licenses/by/4.0/legalcode"^^xsd:string ;
    dcterms:publisher <https://emmc.eu> ;
    dcterms:title "Properties"@en ;
    rdfs:comment "The EMMO requires HermiT reasoner plugin in order to visualize all inferences and class hierarchy (ctrl+R hotkey in Protege)."@en ;
    owl:imports <https://w3id.org/emmo/1.0.2/perspectives/semiotics> ;
    owl:versionIRI <https://w3id.org/emmo/1.0.2/disciplines/properties> ;
    owl:versionInfo "1.0.2"^^xsd:string ;
    ns2:contact "emmo@emmc.eu"^^xsd:string .

