<https://w3id.org/emmo/perspectives/persistence> a owl:Ontology ;
    dcterms:abstract ""@en ;
    dcterms:contributor ns2:GerhardGoldbeck,
        ns2:JesperFriis ;
    dcterms:creator ns2:EmanueleGhedini ;
    dcterms:license "https://creativecommons.org/licenses/by/4.0/legalcode"^^xsd:string ;
    dcterms:publisher <https://emmc.eu> ;
    dcterms:title "Persistence"@en ;
    rdfs:comment ns2:FrancescoZaccarini,
        "The EMMO requires HermiT reasoner plugin in order to visualize all inferences and class hierarchy (ctrl+R hotkey in Protege)."@en ;
    owl:imports <https://w3id.org/emmo/1.0.2/perspectives/perspective> ;
    owl:versionIRI <https://w3id.org/emmo/1.0.2/perspectives/persistence> ;
    owl:versionInfo "1.0.2"^^xsd:string ;
    ns2:contact "emmo@emmc.eu"^^xsd:string .

ns2:Behaviour a owl:Class ;
    rdfs:label "Behaviour"@en ;
    rdfs:comment "A process which is a temporal part of an object."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf ns2:hasBehaviour ] ;
            owl:someValuesFrom ns2:Object ],
        ns2:ProcessByContext ;
    skos:example "Accelerating is a behaviour of a car."@en ;
    skos:prefLabel "Behaviour"@en ;
    ns2:elucidation "A process which is a temporal part of an object."@en .

ns2:Constituent a owl:Class ;
    rdfs:label "Constituent"@en ;
    rdfs:comment "An object which is a spatial part of another object."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf ns2:hasConstituent ] ;
            owl:someValuesFrom ns2:Object ],
        ns2:ObjectByContext ;
    skos:altLabel "ObjectPart"@en ;
    skos:example "A tire is a constituent of a car."@en ;
    skos:prefLabel "Constituent"@en ;
    ns2:elucidation "An object which is a spatial part of another object."@en .

ns2:ConstitutiveProcess a owl:Class ;
    rdfs:label "ConstitutiveProcess"@en ;
    rdfs:comment "A process which is a spatial part of an object."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf ns2:hasConstitutiveProcess ] ;
            owl:someValuesFrom ns2:Object ],
        ns2:ProcessByContext ;
    skos:example "Blood circulation in a human body."@en ;
    skos:prefLabel "ConstitutiveProcess"@en ;
    ns2:elucidation "A process which is a spatial part of an object."@en ;
    ns2:note "A constitutive process is a process that is holistically relevant for the definition of the whole."@en .

ns2:Participant a owl:Class ;
    rdfs:label "Participant"@en ;
    rdfs:comment "An object which is a spatial part of a process."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf ns2:hasParticipant ] ;
            owl:someValuesFrom ns2:Process ],
        ns2:ObjectByContext ;
    skos:example "A student during an examination."@en ;
    skos:prefLabel "Participant"@en ;
    ns2:elucidation "An object which is a spatial part of a process."@en .

ns2:Stage a owl:Class ;
    rdfs:label "Stage"@en ;
    rdfs:comment "A process which is a temporal part of another process."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf ns2:hasStage ] ;
            owl:someValuesFrom ns2:Process ],
        ns2:ProcessByContext ;
    skos:example "Moving a leg is a stage of the process of running."@en ;
    skos:prefLabel "Stage"@en ;
    ns2:elucidation "A process which is a temporal part of another process."@en .

ns2:Status a owl:Class ;
    rdfs:label "Status"@en ;
    rdfs:comment "An object which is a temporal part of a process."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf ns2:hasStatus ] ;
            owl:someValuesFrom ns2:Process ],
        ns2:ObjectByContext ;
    skos:altLabel "State"@en ;
    skos:example "A semi-naked man is a status in the process of a man's dressing."@en ;
    skos:prefLabel "Status"@en ;
    ns2:elucidation "An object which is a temporal part of a process."@en .

ns2:SubObject a owl:Class ;
    rdfs:label "SubObject"@en ;
    rdfs:comment "An object which is a temporal part of another object."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf ns2:hasSubObject ] ;
            owl:someValuesFrom ns2:Object ],
        ns2:ObjectByContext ;
    skos:example "If an inhabited house is considered as an house that is occupied by some people in its majority of time, then an interval of inhabited house in which occasionally nobody is in there is no more an inhabited house, but an unhinabited house, since this temporal part does not satisfy the criteria of the whole."@en ;
    skos:prefLabel "SubObject"@en ;
    ns2:elucidation "An object which is a temporal part of another object."@en ;
    ns2:note "Here we consider a temporal interval that is lower than the characteristic time of the physical process that provides the causality connection between the object parts."@en .

ns2:SubProcess a owl:Class ;
    rdfs:label "SubProcess"@en ;
    rdfs:comment "A process which is a spatial part of another process."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf ns2:hasSubProcess ] ;
            owl:someValuesFrom ns2:Process ],
        ns2:ProcessByContext ;
    skos:example "Breathing is a subprocess of living for a human being."@en ;
    skos:prefLabel "SubProcess"@en ;
    ns2:elucidation "A process which is a spatial part of another process."@en ;
    ns2:note """In the EMMO the relation of participation to a process falls under mereotopology.

Since topological connection means causality, then the only way for a real world object to participate to a process is to be a part of it."""@en .

