@prefix ns2: <http://www.lesfleursdunormal.fr/static/_downloads/owlready_ontology.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:AnalogData a owl:Class ;
    rdfs:label "AnalogData"@en ;
    rdfs:comment "Data that are decoded retaining its continuous variations characteristic."@en ;
    rdfs:subClassOf ns1:DataByDiscretness ;
    skos:example "A vynil contain continuous information about the recorded sound."@en ;
    skos:prefLabel "AnalogData"@en ;
    ns1:elucidation "Data that are decoded retaining its continuous variations characteristic."@en ;
    ns1:note "The fact that there may be a finite granularity in the variations of the material basis (e.g. the smallest peak in a vynil that can be recognized by the piezo-electric transducer) does not prevent a data to be analog. It means only that the focus on such data encoding is on a scale that makes such variations negligible, making them practically a continuum."@en .

ns1:ClassicalData a owl:Class ;
    rdfs:label "ClassicalData"@en ;
    rdfs:comment "Data that are expressed through classical physics mechanisms, having one value and one state, and being in the same place at the same time."@en ;
    rdfs:subClassOf ns1:DataByNature ;
    skos:prefLabel "ClassicalData"@en ;
    ns1:elucidation "Data that are expressed through classical physics mechanisms, having one value and one state, and being in the same place at the same time."@en .

ns1:DiscreteData a owl:Class ;
    rdfs:subClassOf ns1:DataByDiscretness ;
    skos:example "A text is a collection of discrete symbols. A compact disc is designed to host discrete states in the form of pits and lands."@en ;
    skos:prefLabel "DiscreteData"@en ;
    ns1:elucidation "Data whose variations are decoded according to a discrete schema."@en ;
    ns1:note """A discrete schema may be based on a continuum material basis that is filtered according to its variations. For example, a continuous voltage based signal can be considered 1 or 0 according to some threshold.
Discrete does not mean that the material basis is discrete, but that the data are encoded according to such step-based rules."""@en .

ns1:NonNumericalData a owl:Class ;
    rdfs:label "NonNumericalData"@en ;
    rdfs:comment "Data that are non-quantitatively interpreted (e.g., qualitative data, types)."@en ;
    rdfs:subClassOf ns1:DataByNumerical ;
    skos:prefLabel "NonNumericalData"@en ;
    ns1:elucidation "Data that are non-quantitatively interpreted (e.g., qualitative data, types)."@en .

ns1:NumericalData a owl:Class ;
    rdfs:label "NumericalData"@en ;
    rdfs:comment "Data that can be decoded under a quantitative schema and also associated with a graphical number symbols."@en ;
    rdfs:subClassOf ns1:DataByNumerical ;
    skos:prefLabel "NumericalData"@en ;
    ns1:elucidation "Data that can be decoded under a quantitative schema and also associated with a graphical number symbols."@en .

ns1:QuantumData a owl:Class ;
    rdfs:label "QuantumData"@en ;
    rdfs:comment "Data that are expressed through quantum mechanical principles, and that can have several values ​​/ be in several states in the same place at the same time (quantum superposition), each of them with a certain probability."@en ;
    rdfs:subClassOf ns1:DataByNature ;
    skos:prefLabel "QuantumData"@en ;
    ns1:elucidation "Data that are expressed through quantum mechanical principles, and that can have several values ​​/ be in several states in the same place at the same time (quantum superposition), each of them with a certain probability."@en .

ns1:hasDatum a owl:ObjectProperty ;
    rdfs:label "hasDatum"@en ;
    ns2:python_name "hasDatum"@en ;
    rdfs:comment "Relates a dataset to its datum."@en ;
    rdfs:domain ns1:Dataset ;
    rdfs:range ns1:Datum ;
    rdfs:subPropertyOf ns1:hasPortionPart ;
    skos:prefLabel "hasDatum"@en ;
    ns1:elucidation "Relates a dataset to its datum."@en .

: a owl:Ontology ;
    dcterms:abstract ""@en ;
    dcterms:contributor <https://orcid.org/FrancescaBleken>,
