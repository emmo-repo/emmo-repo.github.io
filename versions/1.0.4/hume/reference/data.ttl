    owl:imports <https://w3id.org/emmo/1.0.4/perspectives/contrast> ;
    owl:versionIRI <https://w3id.org/emmo/1.0.4/hume/reference/data> ;
    owl:versionInfo "1.0.4"^^xsd:string ;
    ns1:contact "emmo@emmc.eu"^^xsd:string .

ns1:Dataset a owl:Class ;
    rdfs:label "Dataset"@en ;
    rdfs:comment "Encoded data made of more than one datum."@en ;
    rdfs:subClassOf ns1:DataByStructure ;
    skos:prefLabel "Dataset"@en ;
    ns1:elucidation "Encoded data made of more than one datum."@en .

ns1:Datum a owl:Class ;
    rdfs:label "Datum"@en ;
    rdfs:comment "A self-consistent encoded data entity."@en ;
    rdfs:subClassOf ns1:DataByStructure ;
    skos:example "A character, a bit, a song in a CD."@en ;
    skos:prefLabel "Datum"@en ;
    ns1:elucidation "A self-consistent encoded data entity."@en .

ns1:DataByDiscretness a owl:Class ;
    rdfs:subClassOf ns1:Data ;
    skos:prefLabel "DataByDiscretness"@en ;
    ns1:OWLRL2DLAxioms "EquivalentTo: AnalogData or DiscreteData"^^xsd:string ;
    ns1:elucidation "Categorisation of data by whether it is descrete or not."@en .

ns1:DataByNature a owl:Class ;
    rdfs:label "DataByNature"@en ;
    rdfs:comment "Categorisation of data by its nature."@en ;
    rdfs:subClassOf ns1:Data ;
    skos:prefLabel "DataByNature"@en ;
    ns1:OWLRL2DLAxioms "EquivalentTo: ClassicalData or QuantumData"^^xsd:string ;
    ns1:elucidation "Categorisation of data by its nature."@en .

ns1:DataByNumerical a owl:Class ;
    rdfs:label "DataByNumerical"@en ;
    rdfs:comment "Categorisation of data by whether it is numerical or not."@en ;
    rdfs:subClassOf ns1:Data ;
    skos:prefLabel "DataByNumerical"@en ;
    ns1:OWLRL2DLAxioms "EquivalentTo: NumericalData or NonNumericalData"^^xsd:string ;
    ns1:elucidation "Categorisation of data by whether it is numerical or not."@en .

ns1:DataByStructure a owl:Class ;
    rdfs:label "DataByStructure"@en ;
    rdfs:comment "Categorisation of data by its structure."@en ;
    rdfs:subClassOf ns1:Data ;
    skos:prefLabel "DataByStructure"@en ;
    ns1:OWLRL2DLAxioms "EquivalentTo: Dataset or Datum"^^xsd:string ;
    ns1:elucidation "Categorisation of data by its structure."@en .

