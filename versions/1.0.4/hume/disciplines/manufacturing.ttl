: a owl:Ontology ;
    dcterms:abstract "Describes manufacturing processes with engineered participants. The module also provides a root for engineered materials."@en ;
    dcterms:creator <https://orcid.org/AdhamHashibon>,
        <https://orcid.org/EmanueleGhedini>,
        <https://orcid.org/GeorgSchmitz>,
        <https://orcid.org/GerhardGoldbeck>,
        <https://orcid.org/JesperFriis> ;
    dcterms:license "https://creativecommons.org/licenses/by/4.0/legalcode"^^xsd:string ;
    dcterms:publisher <https://emmc.eu> ;
    dcterms:title "Manufacturing"@en ;
    rdfs:comment "The EMMO requires HermiT reasoner plugin in order to visualize all inferences and class hierarchy (ctrl+R hotkey in Protege)."@en ;
    owl:imports <https://w3id.org/emmo/1.0.4/disciplines/materials>,
        <https://w3id.org/emmo/1.0.4/reference/agency> ;
    owl:versionIRI <https://w3id.org/emmo/1.0.4/hume/disciplines/manufacturing> ;
    owl:versionInfo "1.0.4"^^xsd:string ;
    ns1:contact "emmo@emmc.eu"^^xsd:string .

ns1:3DPrinting a owl:Class ;
    rdfs:label "3DPrinting"@en ;
    rdfs:comment """fabrication of objects through the deposition of a material using a print head, nozzle or another printer technology
Note 1 to entry: This term is often used in a non-technical context synonymously with additive manufacturing (3.1.2) and, in these cases, typically associated with machines used for non-industrial purposes including personal use."""^^xsd:string,
        "Fabrication of objects through the deposition of a material using a print head, nozzle or another printer technology."@en ;
    rdfs:subClassOf ns1:AdditiveManufacturing ;
    skos:prefLabel "3DPrinting"@en ;
    ns1:elucidation "Fabrication of objects through the deposition of a material using a print head, nozzle or another printer technology."@en ;
    ns1:note "This term is often used in a non-technical context synonymously with additive manufacturing and, in these cases, typically associated with machines used for non-industrial purposes including personal use."@en .

ns1:Annealing a owl:Class ;
    rdfs:label "Annealing"@en ;
    rdfs:subClassOf ns1:HeatTreatment ;
    skos:prefLabel "Annealing"@en ;
    ns1:note "heat treatment consisting of heating and soaking at a suitable temperature, followed by cooling under conditions such that, after return to ambient temperature, the metal will be in a structural state closer to that of equilibrium"^^xsd:string .

ns1:Assemblying a owl:Class ;
    rdfs:label "Assemblying"@en ;
    rdfs:comment "The act of connecting together the parts of something"@en ;
    rdfs:subClassOf ns1:JoinManufacturing ;
    skos:prefLabel "Assemblying"@en ;
    ns1:elucidation "The act of connecting together the parts of something"@en ;
    ns1:note "No loss or adds of parts by the components, nor merging. In assemblying parts are losing some of theirs movement degrees of freedom."@en .

ns1:Bending a owl:Class ;
    rdfs:label "Bending"@en ;
    rdfs:comment "Forming of a solid body, whereby the plastic state is essentially brought about by a bending stress"^^xsd:string ;
    rdfs:subClassOf ns1:FlexuralForming,
        ns1:FormingJoin ;
    skos:prefLabel "Bending"@en .

ns1:Cleaning a owl:Class ;
    rdfs:label "Cleaning"@en ;
    rdfs:comment "Process for removing unwanted residual or waste material from a given product or material"^^xsd:string ;
    rdfs:subClassOf ns1:SeparateManufacturing ;
    skos:prefLabel "Cleaning"@en .

ns1:DeepDrawing a owl:Class ;
    rdfs:label "DeepDrawing"@en ;
    rdfs:comment "Forming of vessel parts from a flat mould into a three-dimensional shape by means of a press and tools, whereby material is neither removed nor added"^^xsd:string ;
    rdfs:subClassOf ns1:ReshapeManufacturing,
        ns1:TensileForming ;
    skos:altLabel "Tiefziehen"^^xsd:string ;
    skos:prefLabel "DeepDrawing"@en .

ns1:DeepFreezing a owl:Class ;
    rdfs:label "DeepFreezing"@en ;
    rdfs:subClassOf ns1:HeatTreatment ;
    skos:altLabel "Cryogenic treatment, Deep-freeze"^^xsd:string,
        "Tieftemperaturbehandeln"^^xsd:string ;
    skos:prefLabel "DeepFreezing"@en ;
    ns1:note "Treatment carried out after hardening or case hardening consisting of cooling to a temperature below room temperature to complete the transformation of austenite to martensite"^^xsd:string .

ns1:Dismantling a owl:Class ;
    rdfs:label "Dismantling"@en ;
    rdfs:subClassOf ns1:SeparateManufacturing ;
    skos:altLabel "Demontage"^^xsd:string ;
    skos:prefLabel "Dismantling"@en ;
    ns1:note "action to disassemble a product or a component by removing all or some of its constituent parts with the intent to salvage"^^xsd:string .

ns1:DrawForming a owl:Class ;
    rdfs:label "DrawForming"@en ;
    rdfs:comment "Draw forming by drawing a workpiece through a tool opening that is narrowed in the drawing direction."^^xsd:string ;
    rdfs:subClassOf ns1:TensileForming ;
    skos:prefLabel "DrawForming"@en .

ns1:Drilling a owl:Class ;
    rdfs:label "Drilling"@en ;
    rdfs:comment "machining with a circular cutting movement in which the axis of rotation of the tool and the axis of the internal surface to be produced are identical and the feed movement is in the direction of this axis. The axis of rotation of the cutting movement maintains its position relative to the workpiece independently of the feed movement (axis of rotation workpiece-bound)."^^xsd:string ;
    rdfs:subClassOf ns1:Machining ;
    skos:altLabel "Bohren"^^xsd:string ;
    skos:prefLabel "Drilling"@en .

ns1:Factory a owl:Class ;
    rdfs:label "Factory"@en ;
    rdfs:comment "A building or group of buildings where goods are manufactured or assembled."@en ;
    rdfs:subClassOf ns1:HolisticSystem ;
    skos:altLabel "IndustrialPlant"@en ;
    skos:prefLabel "Factory"@en ;
    ns1:elucidation "A building or group of buildings where goods are manufactured or assembled."@en .

ns1:FormingBlasting a owl:Class ;
    rdfs:label "FormingBlasting"@en ;
    rdfs:comment "Shot peening is shot peening for shaping or straightening workpieces by introducing residual compressive stresses (from: DIN 8200/10.82)."^^xsd:string ;
    rdfs:subClassOf ns1:MaterialTreatment ;
    skos:altLabel "Umformstrahlen"^^xsd:string ;
    skos:prefLabel "FormingBlasting"@en .

ns1:FormingFromGas a owl:Class ;
    rdfs:label "FormingFromGas"@en ;
    rdfs:subClassOf ns1:FromNotProperShapeToWorkPiece ;
    skos:prefLabel "FormingFromGas"@en .

ns1:FormingFromIonised a owl:Class ;
