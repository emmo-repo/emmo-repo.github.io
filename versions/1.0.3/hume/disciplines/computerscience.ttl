@prefix : <https://w3id.org/emmo/hume/disciplines/computerscience#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ns1: <https://w3id.org/emmo#> .
@prefix ns2: <http://www.lesfleursdunormal.fr/static/_downloads/owlready_ontology.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

rdf:HTML a rdfs:Datatype .

owl:qualifiedCardinality a owl:AnnotationProperty .

ns1:ApplicationSpecificScript a owl:Class ;
    rdfs:label "ApplicationSpecificScript"@en ;
    rdfs:comment "A scripting language developed specifically for an application, so that it's usage and interpretation is limited in this context."^^xsd:string ;
    rdfs:subClassOf ns1:ScriptingLanguage ;
    skos:example "Scripting file for the execution of modelling software such as LAMMPS, OpenFOAM, or for general purpose platforms such as MATLAB or Mathematica."^^xsd:string ;
    skos:prefLabel "ApplicationSpecificScript"@en ;
    ns1:elucidation "A scripting language developed specifically for an application, so that it's usage and interpretation is limited in this context."^^xsd:string .

ns1:C a owl:Class ;
    rdfs:label "C"@en ;
    rdfs:subClassOf ns1:CompiledLanguage ;
    skos:prefLabel "C"@en .

ns1:CPlusPlus a owl:Class ;
    rdfs:label "CPlusPlus"@en ;
    rdfs:comment "A language object respecting the syntactic rules of C++."@en ;
    rdfs:subClassOf ns1:CompiledLanguage ;
    skos:altLabel "C++"@en ;
    skos:prefLabel "CPlusPlus"@en ;
    ns1:elucidation "A language object respecting the syntactic rules of C++."@en .

ns1:CSharp a owl:Class ;
    rdfs:label "CSharp"@en ;
    rdfs:subClassOf ns1:CompiledLanguage ;
    skos:altLabel "C#"@en ;
    skos:prefLabel "CSharp"@en .

ns1:Command a owl:Class ;
    rdfs:label "Command"@en ;
    rdfs:comment "An instruction to a computer system to perform a given task."@en ;
    rdfs:subClassOf ns1:Workflow ;
    skos:example "From a bash shell would e.g. `ls` be a command. Another example of a shell command would be `/path/to/executable arg1 arg2`."@en ;
    skos:prefLabel "Command"@en ;
    ns1:note "A command must be interpretable by the computer system."@en,
        "Commands are typically performed from a shell or a shell script, but not limited to them."@en .

ns1:ConfigurationLanguage a owl:Class ;
    rdfs:label "ConfigurationLanguage"@en ;
    rdfs:comment "A construction language used to write configuration files."@en ;
    rdfs:subClassOf ns1:ConstructionLanguage ;
    skos:example ".ini files"@en,
        "Files in the standard .config directory on Unix systems."@en ;
    skos:prefLabel "ConfigurationLanguage"@en ;
    ns1:elucidation "A construction language used to write configuration files."@en ;
    ns1:wikipediaReference "https://en.wikipedia.org/wiki/Configuration_file#Configuration_languages"^^xsd:anyURI .

ns1:DataExchangeLanguage a owl:Class ;
    rdfs:label "DataExchangeLanguage"@en ;
    rdfs:comment "A computer language that is domain-independent and can be used for expressing data from any kind of discipline."@en ;
    rdfs:subClassOf ns1:ComputerLanguage ;
    skos:example "JSON, YAML, XML"@en ;
    skos:prefLabel "DataExchangeLanguage"@en ;
    ns1:elucidation "A computer language that is domain-independent and can be used for expressing data from any kind of discipline."@en ;
    ns1:wikipediaReference "https://en.wikipedia.org/wiki/Data_exchange#Data_exchange_languages"^^xsd:anyURI .

ns1:Database a owl:Class ;
    rdfs:label "Database"@en ;
    rdfs:comment "A structured collection of data held in a computer system that has a well-defined interface."@en ;
    rdfs:subClassOf ns1:DigitalData ;
    skos:example """- A database covering nine million chemical compositions.
- A thermodynamic database."""@en ;
    skos:prefLabel "Database"@en ;
    ns1:elucidation "A structured collection of data held in a computer system that has a well-defined interface."@en .

ns1:FORTRAN a owl:Class ;
    rdfs:label "FORTRAN"@en ;
    rdfs:subClassOf ns1:CompiledLanguage ;
    skos:prefLabel "FORTRAN"@en .

ns1:File a owl:Class ;
    rdfs:label "File"@en ;
    rdfs:comment "In computing, a computer file is a resource for recording data on a computer storage device, primarily identified by its file path."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ns1:hasResourceIdentifier ;
            owl:someValuesFrom ns1:Path ],
        ns1:DigitalData,
        ns1:SystemResource ;
    skos:prefLabel "File"@en ;
    ns1:elucidation "In computing, a computer file is a resource for recording data on a computer storage device, primarily identified by its file path."@en .

ns1:Java a owl:Class ;
    rdfs:label "Java"@en ;
    rdfs:subClassOf ns1:CompiledLanguage ;
    skos:prefLabel "Java"@en .

ns1:JavaScript a owl:Class ;
    rdfs:label "JavaScript"@en ;
    rdfs:subClassOf ns1:ScriptingLanguage ;
    skos:prefLabel "JavaScript"@en .

ns1:MarkupLanguage a owl:Class ;
    rdfs:label "MarkupLanguage"@en ;
    rdfs:comment "A grammar for annotating a document in a way that is syntactically distinguishable from the text."@en ;
    rdfs:subClassOf ns1:ComputerLanguage ;
    skos:example "HTML"@en ;
    skos:prefLabel "MarkupLanguage"@en ;
    ns1:elucidation "A grammar for annotating a document in a way that is syntactically distinguishable from the text."@en ;
    ns1:wikipediaReference "https://en.wikipedia.org/wiki/Markup_language"^^xsd:anyURI .

ns1:Python a owl:Class ;
    rdfs:label "Python"@en ;
    rdfs:subClassOf ns1:ScriptingLanguage ;
    skos:prefLabel "Python"@en .

ns1:QueryLanguage a owl:Class ;
    rdfs:label "QueryLanguage"@en ;
    rdfs:comment "A construction language used to make queries in databases and information systems."@en ;
    rdfs:subClassOf ns1:ConstructionLanguage ;
    skos:example "SQL, SPARQL"@en ;
    skos:prefLabel "QueryLanguage"@en ;
    ns1:elucidation "A construction language used to make queries in databases and information systems."@en ;
    ns1:wikipediaReference "https://en.wikipedia.org/wiki/Query_language"^^xsd:anyURI .

ns1:Ruby a owl:Class ;
    rdfs:label "Ruby"@en ;
    rdfs:subClassOf ns1:ScriptingLanguage ;
    skos:prefLabel "Ruby"@en .

ns1:ShellScript a owl:Class ;
    rdfs:label "ShellScript"@en ;
    rdfs:comment "A command language designed to be run by a command-line interpreter, like a Unix shell."@en ;
    rdfs:subClassOf ns1:CommandLanguage ;
    skos:prefLabel "ShellScript"@en ;
    ns1:elucidation "A command language designed to be run by a command-line interpreter, like a Unix shell."@en ;
    ns1:wikipediaReference "https://en.wikipedia.org/wiki/Shell_script"^^xsd:string .

ns1:SimulationLanguage a owl:Class ;
    rdfs:label "SimulationLanguage"@en ;
    rdfs:comment "A computer language used to describe simulations."@en ;
    rdfs:subClassOf ns1:ModellingLanguage ;
    skos:prefLabel "SimulationLanguage"@en ;
    ns1:elucidation "A computer language used to describe simulations."@en ;
    ns1:wikipediaReference "https://en.wikipedia.org/wiki/Simulation_language"^^xsd:anyURI .

ns1:SpecificationLanguage a owl:Class ;
    rdfs:label "SpecificationLanguage"@en ;
    rdfs:comment "A language used to describe what a computer system should do."@en ;
    rdfs:subClassOf ns1:ComputerLanguage ;
    skos:example "ACSL, VDM, LOTUS, MML, ..."@en ;
    skos:prefLabel "SpecificationLanguage"@en ;
    ns1:elucidation "A language used to describe what a computer system should do."@en ;
    ns1:wikipediaReference "https://en.wikipedia.org/wiki/Specification_language"^^xsd:anyURI .

ns1:StyleSheetLanguage a owl:Class ;
    rdfs:label "StyleSheetLanguage"@en ;
    rdfs:comment "A computer language that expresses the presentation of structured documents."@en ;
    rdfs:subClassOf ns1:ComputerLanguage ;
    skos:example "CSS"@en ;
    skos:prefLabel "StyleSheetLanguage"@en ;
    ns1:elucidation "A computer language that expresses the presentation of structured documents."@en ;
    ns1:wikipediaReference "https://en.wikipedia.org/wiki/Style_sheet_language"^^xsd:anyURI .

ns1:TransformationLanguage a owl:Class ;
    rdfs:label "TransformationLanguage"@en ;
    rdfs:comment "A construction language designed to transform some input text in a certain formal language into a modified output text that meets some specific goal."@en ;
    rdfs:subClassOf ns1:ConstructionLanguage ;
    skos:example "Tritium, XSLT, XQuery, STX, FXT, XDuce, CDuce, HaXml, XMLambda, FleXML"@en ;
    skos:prefLabel "TransformationLanguage"@en ;
    ns1:elucidation "A construction language designed to transform some input text in a certain formal language into a modified output text that meets some specific goal."@en ;
    ns1:wikipediaReference "https://en.wikipedia.org/wiki/Transformation_language"^^xsd:anyURI .

ns1:hasJSONValue a owl:DatatypeProperty ;
    rdfs:label "hasJSONValue"@en ;
    ns2:python_name "hasJSONValue"@en ;
    rdfs:comment "The owl:dataProperty that provides a serialisation of an EMMO JSON data entity."@en ;
    rdfs:domain ns1:JSONData ;
    rdfs:range rdf:JSON ;
    rdfs:subPropertyOf ns1:hasDataValue ;
    skos:prefLabel "hasJSONValue"@en ;
    ns1:elucidation "The owl:dataProperty that provides a serialisation of an EMMO JSON data entity."@en .

ns1:hasURLValue a owl:DatatypeProperty ;
    rdfs:label "hasURLValue"@en ;
    ns2:python_name "hasURLValue"@en ;
    rdfs:comment "The owl:dataProperty that provides a serialisation of an EMMO URL data entity."@en ;
    rdfs:domain ns1:URL ;
    rdfs:range xsd:anyURI ;
    rdfs:subPropertyOf ns1:hasURIValue ;
    skos:prefLabel "hasURLValue"@en ;
    ns1:elucidation "The owl:dataProperty that provides a serialisation of an EMMO URL data entity."@en .

ns1:hasURNValue a owl:DatatypeProperty ;
    rdfs:label "hasURNValue"@en ;
    ns2:python_name "hasURNValue"@en ;
    rdfs:comment "The owl:dataProperty that provides a serialisation of an EMMO URN data entity."@en ;
    rdfs:domain ns1:URN ;
    rdfs:range xsd:anyURI ;
    rdfs:subPropertyOf ns1:hasURIValue ;
    skos:prefLabel "hasURNValue"@en ;
    ns1:elucidation "The owl:dataProperty that provides a serialisation of an EMMO URN data entity."@en .

: a owl:Ontology ;
    dcterms:abstract "Key concepts from the domain of computer science."@en ;
    dcterms:contributor <https://orcid.org/FrancescaBleken>,
        <https://orcid.org/OtelloRoscioni> ;
    dcterms:creator <https://orcid.org/EmanueleGhedini>,
        <https://orcid.org/GerhardGoldbeck>,
        <https://orcid.org/JesperFriis> ;
    dcterms:license "https://creativecommons.org/licenses/by/4.0/legalcode"^^xsd:string ;
    dcterms:publisher <https://emmc.eu> ;
    dcterms:title "Computer science"@en ;
    rdfs:comment "The EMMO requires HermiT reasoner plugin in order to visualize all inferences and class hierarchy (ctrl+R hotkey in Protege)."@en ;
    owl:imports <https://w3id.org/emmo/1.0.3/perspectives/semiotics>,
        <https://w3id.org/emmo/1.0.3/reference/symbolic>,
        <https://w3id.org/emmo/1.0.3/reference/workflow> ;
    owl:versionIRI <https://w3id.org/emmo/1.0.3/hume/disciplines/computerscience> ;
    owl:versionInfo "1.0.3"^^xsd:string ;
    ns1:contact "emmo@emmc.eu"^^xsd:anyURI .

ns1:ApplicationProgram a owl:Class ;
    rdfs:subClassOf ns1:Software ;
    skos:altLabel "App"@en,
        "Application"@en ;
    skos:example "Word processors, graphic image processing programs, database management systems, numerical simulation software and games."@en ;
    skos:prefLabel "ApplicationProgram"@en ;
    ns1:elucidation "A program aimed to provide a specific high level function to the user, usually hiding lower level procedures."@en .

ns1:CommandLanguage a owl:Class ;
    rdfs:label "CommandLanguage"@en ;
    rdfs:comment "An interpreted computer language for job control in computing."@en ;
    rdfs:subClassOf ns1:ConstructionLanguage ;
    skos:example """Unix shell.
Batch programming languages."""@en ;
    skos:prefLabel "CommandLanguage"@en ;
    ns1:elucidation "An interpreted computer language for job control in computing."@en ;
    ns1:wikipediaReference "https://en.wikipedia.org/wiki/Command_language"^^xsd:anyURI .

ns1:ComputerSystem a owl:Class ;
    rdfs:label "ComputerSystem"@en ;
    rdfs:comment "Electronic device capable of processing data, typically in binary form, according to instructions given to it in a variable program."@en ;
    rdfs:subClassOf ns1:Object ;
    skos:altLabel "Computer"@en ;
    skos:prefLabel "ComputerSystem"@en ;
    ns1:elucidation "Electronic device capable of processing data, typically in binary form, according to instructions given to it in a variable program."@en ;
    ns1:wikipediaReference "https://en.wikipedia.org/wiki/Computer"^^xsd:anyURI .

ns1:LanguageData a owl:Class ;
    rdfs:label "LanguageData"@en ;
    rdfs:subClassOf ns1:TokenData ;
    skos:prefLabel "LanguageData"@en .

ns1:ModellingLanguage a owl:Class ;
    rdfs:label "ModellingLanguage"@en ;
    rdfs:comment "An artificial computer language used to express information or knowledge, often for use in computer system design."@en ;
    rdfs:subClassOf ns1:ComputerLanguage ;
    skos:example "Architecture description language – used as a language (or a conceptual model) to describe and represent system architectures."@en,
        """Hardware description language – used to model integrated circuits.

Architecture description language – used as a language (or a conceptual model) to describe and represent system architectures.

Algebraic Modeling Language which is a high-level programming languages for describing and solving high complexity problems like large-scale optimisation."""@en ;
    skos:prefLabel "ModellingLanguage"@en ;
    ns1:elucidation "An artificial computer language used to express information or knowledge, often for use in computer system design."@en ;
    ns1:wikipediaReference "https://en.wikipedia.org/wiki/Modeling_language"^^xsd:anyURI .

ns1:NCNameData a owl:Class ;
    rdfs:label "NCNameData"@en ;
    rdfs:subClassOf ns1:NameData ;
    skos:prefLabel "NCNameData"@en .

ns1:NMTOKENData a owl:Class ;
    rdfs:label "NMTOKENData"@en ;
    rdfs:subClassOf ns1:TokenData ;
    skos:prefLabel "NMTOKENData"@en .

ns1:Program a owl:Class ;
    rdfs:label "Program"@en ;
    rdfs:comment "A set of instructions that tell a computer what to do."@en ;
    rdfs:subClassOf ns1:Software ;
    owl:equivalentClass [ a owl:Class ;
            owl:unionOf ( ns1:ApplicationProgram ns1:SystemProgram ) ] ;
    skos:altLabel "Executable"@en ;
    skos:prefLabel "Program"@en ;
    ns1:elucidation "A set of instructions that tell a computer what to do."@en ;
    ns1:note "A program is a sequence of instructions understandable by a computer's central processing unit (CPU) that indicates which operations the computer should perform on a set of data."@en .

ns1:SourceCode a owl:Class ;
    rdfs:label "SourceCode"@en ;
    rdfs:comment "A programming language entity expressing a formal detailed plan of what a software is intended to do."@en ;
    rdfs:subClassOf ns1:ProgrammingLanguage,
        ns1:Software ;
    skos:prefLabel "SourceCode"@en ;
    ns1:elucidation "A programming language entity expressing a formal detailed plan of what a software is intended to do."@en ;
    ns1:note "A source code is the companion of an application, being it the entity used to generate the application list of CPU executable instructions."@en,
        "Source code (also referred to as source or code) is the version of software as it is originally written (i.e., typed into a computer) by a human in plain text (i.e., human readable alphanumeric characters)."@en .

ns1:SystemProgram a owl:Class ;
    rdfs:label "SystemProgram"@en ;
    rdfs:comment "System program refers to operating systems and utility programs that manage computer resources at a low level enabling a computer to function."@en ;
    rdfs:subClassOf ns1:Software ;
    skos:example "An operating system. A graphic driver."@en ;
    skos:prefLabel "SystemProgram"@en ;
    ns1:elucidation "System program refers to operating systems and utility programs that manage computer resources at a low level enabling a computer to function."@en .

ns1:hasResourceIdentifier a owl:ObjectProperty ;
    rdfs:label "hasResourceIdentifier"@en ;
    ns2:python_name "hasResourceIdentifier"@en ;
    rdfs:comment "Relates a resource to its identifier."@en ;
    rdfs:range ns1:ResourceIdentifier ;
    rdfs:subPropertyOf ns1:hasConvention ;
    skos:prefLabel "hasResourceIdentifier"@en ;
    ns1:elucidation "Relates a resource to its identifier."@en .

rdf:JSON a rdfs:Datatype .

ns1:IRI a owl:Class ;
    rdfs:label "IRI"@en ;
    rdfs:comment "An Internationalized Resource Identifier (IRI) is a compact sequence of characters that identifies an abstract or physical resource. It is similar to URI, but greatly extends the allowed character set from ASCII to the Universal Character Set (Unicode/ISO 10646).."@en ;
    rdfs:subClassOf ns1:ResourceIdentifier,
        ns1:SymbolicData ;
    skos:example "https://en.wiktionary.org/wiki/Ῥόδος"^^xsd:anyURI ;
    skos:prefLabel "IRI"@en ;
    ns1:elucidation "An Internationalized Resource Identifier (IRI) is a compact sequence of characters that identifies an abstract or physical resource. It is similar to URI, but greatly extends the allowed character set from ASCII to the Universal Character Set (Unicode/ISO 10646).."@en ;
    ns1:note "IRIs are commonly used as identifiers for ontological entities, although the extended unicode character set is rarely used."@en ;
    ns1:wikipediaReference "https://en.wikipedia.org/wiki/Internationalized_Resource_Identifier"^^xsd:anyURI .

ns1:JSONData a owl:Class ;
    rdfs:label "JSONData"@en ;
    rdfs:subClassOf ns1:ComputerScience,
        ns1:SymbolicData ;
    skos:prefLabel "JSONData"@en .

ns1:NameData a owl:Class ;
    rdfs:label "NameData"@en ;
    rdfs:subClassOf ns1:TokenData ;
    skos:prefLabel "NameData"@en .

ns1:NormalizedStringData a owl:Class ;
    rdfs:label "NormalizedStringData"@en ;
    rdfs:subClassOf ns1:StringData ;
    skos:prefLabel "NormalizedStringData"@en .

ns1:Path a owl:Class ;
    rdfs:label "Path"@en ;
    rdfs:comment "A path is a string of characters used to uniquely identify a location in a directory structure according to a particular convention."@en ;
    rdfs:subClassOf ns1:ResourceIdentifier,
        ns1:String ;
    skos:example """/etc/fstab  (UNIX-like path)
C:\\\\Users\\\\John\\\\Desktop  (DOS-like path)"""@en ;
    skos:prefLabel "Path"@en ;
    ns1:elucidation "A path is a string of characters used to uniquely identify a location in a directory structure according to a particular convention."@en .

ns1:SystemResource a owl:Class ;
    rdfs:label "SystemResource"@en ;
    rdfs:comment "Any physical or virtual component of limited availability within a computer system."@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( ns1:Item [ a owl:Restriction ;
                        owl:onProperty [ owl:inverseOf ns1:hasPortionPart ] ;
                        owl:someValuesFrom ns1:ComputerSystem ] ) ],
        ns1:Object ;
    skos:altLabel "Resource"@en ;
    skos:prefLabel "SystemResource"@en ;
    ns1:elucidation "Any physical or virtual component of limited availability within a computer system."@en .

ns1:URL a owl:Class ;
    rdfs:label "URL"@en ;
    rdfs:comment "The term \"Uniform Resource Locator\" (URL) refers to the subset of URIs that, in addition to identifying a resource, provide a means of locating the resource by describing its primary access mechanism (e.g., its network \"location\")."@en ;
    rdfs:subClassOf ns1:URI ;
    skos:prefLabel "URL"@en ;
    ns1:elucidation "The term \"Uniform Resource Locator\" (URL) refers to the subset of URIs that, in addition to identifying a resource, provide a means of locating the resource by describing its primary access mechanism (e.g., its network \"location\")."@en .

ns1:URN a owl:Class ;
    rdfs:label "URN"@en ;
    rdfs:comment "The term \"Uniform Resource Name\" (URN) has been used historically to refer to both URIs under the \"urn\" scheme [RFC2141], which are required to remain globally unique and persistent even when the resource ceases to exist or becomes unavailable, and to any other URI with the properties of a name."@en ;
    rdfs:subClassOf ns1:URI ;
    skos:prefLabel "URN"@en ;
    ns1:elucidation "The term \"Uniform Resource Name\" (URN) has been used historically to refer to both URIs under the \"urn\" scheme [RFC2141], which are required to remain globally unique and persistent even when the resource ceases to exist or becomes unavailable, and to any other URI with the properties of a name."@en .

ns1:hasURIValue a owl:DatatypeProperty ;
    rdfs:label "hasURIValue"@en ;
    ns2:python_name "hasURIValue"@en ;
    rdfs:comment "The owl:dataProperty that provides a serialisation of an EMMO URI data entity."@en ;
    rdfs:domain ns1:URI ;
    rdfs:range xsd:anyURI ;
    rdfs:subPropertyOf ns1:hasDataValue ;
    skos:prefLabel "hasURIValue"@en ;
    ns1:elucidation "The owl:dataProperty that provides a serialisation of an EMMO URI data entity."@en .

ns1:ProgrammingLanguage a owl:Class ;
    rdfs:label "ProgrammingLanguage"@en ;
    rdfs:comment "A language object that follows syntactic rules of a programming language."@en ;
    rdfs:subClassOf ns1:ConstructionLanguage ;
    skos:altLabel "Code"@en,
        "SoftwareCode"@en ;
    skos:prefLabel "ProgrammingLanguage"@en ;
    ns1:elucidation "A language object that follows syntactic rules of a programming language."@en ;
    ns1:note "A programming language object can also be a fragment (e.g. a C function) not suitable for exectution."@en,
        "Entities are not necessarily digital data, but can be code fragments printed on paper."@en .

ns1:ResourceIdentifier a owl:Class ;
    rdfs:label "ResourceIdentifier"@en ;
    rdfs:comment "A formal computer-interpretable identifier of a system resource."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf ns1:hasConvention ] ;
            owl:someValuesFrom ns1:SystemResource ],
        ns1:ComputerScience ;
    skos:prefLabel "ResourceIdentifier"@en ;
    ns1:elucidation "A formal computer-interpretable identifier of a system resource."@en .

ns1:ComputerScience a owl:Class ;
    rdfs:label "ComputerScience"@en ;
    rdfs:comment "A well-formed formula that follows the syntactic rules of computer science."@en ;
    rdfs:subClassOf ns1:Language ;
    skos:prefLabel "ComputerScience"@en ;
    ns1:elucidation "A well-formed formula that follows the syntactic rules of computer science."@en ;
    ns1:note "A well-formed formula in computer science may be or not be interpreted by a computer. For example pseudo-code is only intended for human consumption."@en .

ns1:DigitalData a owl:Class ;
    rdfs:subClassOf ns1:DiscreteData ;
    skos:altLabel "BinaryData"@en ;
    skos:prefLabel "DigitalData"@en ;
    ns1:elucidation "Discrete data that are decoded as a sequence of 1/0, or true/false, or on/off."@en .

ns1:ScriptingLanguage a owl:Class ;
    rdfs:label "ScriptingLanguage"@en ;
    rdfs:comment "A programming language that is executed through runtime interpretation."@en ;
    rdfs:subClassOf ns1:ProgrammingLanguage ;
    skos:prefLabel "ScriptingLanguage"@en ;
    ns1:elucidation "A programming language that is executed through runtime interpretation."@en .

ns1:TokenData a owl:Class ;
    rdfs:label "TokenData"@en ;
    rdfs:subClassOf ns1:NormalizedStringData ;
    skos:prefLabel "TokenData"@en .

ns1:CompiledLanguage a owl:Class ;
    rdfs:label "CompiledLanguage"@en ;
    rdfs:subClassOf ns1:ProgrammingLanguage ;
    skos:prefLabel "CompiledLanguage"@en .

ns1:ConstructionLanguage a owl:Class ;
    rdfs:label "ConstructionLanguage"@en ;
    rdfs:comment "A computer language by which a human can specify an executable problem solution to a computer."@en ;
    rdfs:subClassOf ns1:ComputerLanguage ;
    skos:prefLabel "ConstructionLanguage"@en ;
    ns1:elucidation "A computer language by which a human can specify an executable problem solution to a computer."@en ;
    ns1:wikipediaReference "https://en.wikipedia.org/wiki/Software_construction#Construction_languages"^^xsd:anyURI .

ns1:URI a owl:Class ;
    rdfs:label "URI"@en ;
    rdfs:comment "A Uniform Resource Identifier (URI) is a compact sequence of characters that identifies an abstract or physical resource."@en ;
    rdfs:subClassOf ns1:ComputerScience,
        ns1:IRI ;
    skos:prefLabel "URI"@en ;
    ns1:elucidation "A Uniform Resource Identifier (URI) is a compact sequence of characters that identifies an abstract or physical resource."@en ;
    ns1:illustration "https://en.wikipedia.org/wiki/File:URI_syntax_diagram.svg"^^xsd:anyURI ;
    ns1:note "URI = scheme \":\" [\"//\" authority] path [\"?\" query] [\"#\" fragment]"@en .

ns1:ComputerLanguage a owl:Class ;
    rdfs:label "ComputerLanguage"@en ;
    rdfs:comment "A formal language used to communicate with a computer."@en ;
    rdfs:subClassOf ns1:ComputerScience ;
    skos:prefLabel "ComputerLanguage"@en ;
    ns1:elucidation "A formal language used to communicate with a computer."@en ;
    ns1:note """The categorisation of computer languages is based on

