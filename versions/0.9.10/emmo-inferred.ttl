            owl:onProperty <http://emmo.info/0.9.10/perspectives/emmo-existent.owl#EMMO_b2282816_b7a3_44c6_b2cb_3feff1ceb7fe> ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        <http://emmo.info/0.9.10/domains/emmo-material.owl#EMMO_5c4aff3c_c30c_4507_86d5_b4df41eb9f2f> .

